		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.removeOneFromRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.addOneToRoom();
		Kitchen.removeOneFromRoom();
		int total = Kitchen.getTotal();
		int roomNum = Kitchen.getNumberInRoom();
		System.out.println("The number in Kitchen is " + roomNum);
		System.out.println("The total is " + total);
		
		
